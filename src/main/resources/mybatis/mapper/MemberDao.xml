<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="Spboot.sroom.dao.IMemberDao">
	<select id="searchMember" resultType="String">
		select mem_id from sr_member where mem_id=#{param1}
	</select>
	<insert id="insertMember">
		insert into sr_member(mem_id,mem_name,mem_nickname,mem_email,mem_age,mem_gender,mem_image) 
		values(#{id},#{name},#{nickname},#{email},#{age},#{gender},#{image})
	</insert>

</mapper>